@// $Id: 12-elseif.oc,v 1.1 2019-04-16 12:00:09-07 - - $

#include "oclib.h"

int main (int argc) {
   if (argc == 1) putstr ("one");
   else if (argc == 2) putstr ("two");
   else if (argc == 3) putstr ("three");
   else putstr ("many");
   putchr ('\n');
   return SUCCESS;
}

