@// $Id: 11-numbers.oc,v 1.1 2019-04-16 12:14:45-07 - - $

#include "oclib.h"

int main() {
   int number = 1;
   BOOL done = FALSE;
   while (not done) {
      putint (number);
      putchr ('\n');
      if (number <= 0) done = TRUE;
      number = number + number;
   }
   return SUCCESS;
}

